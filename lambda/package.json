{
  "name": "lambda",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "typescript": "^4.9.4"
  },
  "scripts": {
    "build": "tsc",
    "esbuild": "esbuild --bundle src/my-lambda.ts --target=node16 --format=cjs --external:aws-sdk --outfile=build/index.js",
    "watch": "tsc -w",
    "zip": "(cd build && zip package.zip index.js )",
    "bundle": "yarn esbuild && yarn zip"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.226.0",
    "@types/aws-lambda": "^8.10.109",
    "@types/node": "^18.11.18",
    "esbuild": "^0.16.16"
  }
}
